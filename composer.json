{
  "name": "liangtao/etcd-php",
  "type": "library",
  "description": "PHP client for Etcd v3.5",
  "keywords": [
    "etcdv3",
    "etcd",
    "etcd-client",
    "etcd-php"
  ],
  "homepage": "https://github.com/liangtao-top/etcd-php",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "TaoGe",
      "email": "liangtao.gz@foxmail.com",
      "role": "Developer",
      "homepage": "https://github.com/liangtao-top"
    }
  ],
  "require": {
    "php": ">= 8.0.12",
    "guzzlehttp/guzzle": "^7.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  },
  "autoload": {
    "psr-4": {
      "Etcd\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Etcd\\Tests\\": "tests/"
    }
  },
  "repositories": [
    {
      "type": "path",
      "url": "../../com/php/core"
    },
    {
      "type": "composer",
      "url": "https://mirrors.aliyun.com/composer/"
    }
  ]
}
